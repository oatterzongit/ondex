<div>

  <% @auctions.each do |auction| %>

    <div class="">
      <h5><%= link_to auction.item_name, auction %></h5>
      <article class="">
        <%= auction.item_desc %>
      </article>
      <br>
      <div class="">
        <img src="<%= auction.item_img(:thumb) %>" alt="item_image"/>
      </div>
    </div>

  <% end %>

</div>
